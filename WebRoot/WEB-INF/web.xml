    <servlet-name>indexservlet</servlet-name>
    <servlet-class>com.servlet.indexservlet</servlet-class>
  </servlet>











  <servlet-mapping>
    <servlet-name>insertManager</servlet-name>
    <url-pattern>/admin/insertManager</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>edit</servlet-name>
    <url-pattern>/admin/edit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>del</servlet-name>
    <url-pattern>/admin/del</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>logservlet</servlet-name>
    <url-pattern>/logservlet</url-pattern>
  </servlet-mapping>
