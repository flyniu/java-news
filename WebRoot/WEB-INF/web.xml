    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Insertartical</servlet-name>
    <servlet-class>com.servlet.Insertartical</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>editartical</servlet-name>
    <servlet-class>com.servlet.editartical</servlet-class>
  </servlet>
  <servlet>
