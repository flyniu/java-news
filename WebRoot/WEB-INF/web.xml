  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>del</servlet-name>
    <servlet-class>com.servlet.del</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>logservlet</servlet-name>
    <servlet-class>com.servlet.logservlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>insertnav</servlet-name>
    <servlet-class>com.servlet.insertnav</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
